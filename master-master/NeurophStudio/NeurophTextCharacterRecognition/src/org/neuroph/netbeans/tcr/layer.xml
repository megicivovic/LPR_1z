<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-neuroph-netbeans-tcr-TCRAction.instance">
                <attr name="component" methodvalue="org.neuroph.netbeans.tcr.TCRTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.neuroph.netbeans.tcr.Bundle#CTL_TCRAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
            
        </folder>
    </folder>
    
    <folder name="Templates">
        <folder name="Neuroph">      
            <file name="tCR">
                <attr name="displayName" bundlevalue="org.neuroph.netbeans.tcr.wiz.Bundle#Templates/Neuroph/tCR"/>
                <attr name="instantiatingIterator" newvalue="org.neuroph.netbeans.tcr.wiz.TCRWizardIterator"/>
                <attr name="position" intvalue="250"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/org/neuroph/netbeans/tcr/wiz/TextRecognition.html"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/neuroph/netbeans/tcr/wiz/tcrIcon.png"/>
            </file>
        </folder>
    </folder>    
    
    <folder name="Windows2">
        <folder name="Components">
            <file name="TCRTopComponent.settings" url="TCRTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="TCRTopComponent.wstcref" url="TCRTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
